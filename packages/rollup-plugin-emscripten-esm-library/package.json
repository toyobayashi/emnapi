  },
  "homepage": "https://github.com/toyobayashi/emnapi/tree/main/packages/rollup-plugin-emscripten-esm-library#readme",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "rollup": ">= 2.0.0"
  },
  "dependencies": {
    "@emnapi/ts-transform-emscripten-esm-library": "^1.0.0"
