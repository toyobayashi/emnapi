{
  "extends": "../shared/tsconfig.base.json",
  "compilerOptions": {
    "target": "ES5",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "importHelpers": false,
    "noEmitHelpers": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "types": [],
    "lib": [
      "ES5",
      "ES2015",
      "ES2020.BigInt",
      "ES2021.WeakRef",
      "ES2017.SharedMemory",
      "DOM"
    ]
  }
}
